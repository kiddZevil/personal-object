
<!--右侧主体内容begin-->
<div class="wrap_right_con pt-0">
    <div class="all_order add-channel border-0">
        <form class="form-horizontal form-err-tips">
            <!--渠道名称 beg-->
            <div class="control-group">
                <div class="control-label">
                    <em class="con-required">*</em>
                    渠道名称:
                </div>
                <div class="controls">
                    <input type="text" name="channel-name" placeholder="请输入渠道名称"  class="mktgChnlNm" MAXLENGTH="20">
                </div>
                <div id="J-channel-name_wrapper" class="error-tips fn-hide">
                    <p id="J-channel-name" class="error">渠道名称为4-20位字符组成</p>
                </div>
            </div>
            <!--渠道名称  end-->
            <!--渠道编号 beg-->
            <div class="control-group">
                <div class="control-label">
                    <em class="con-required">*</em>
                    渠道编号:
                </div>
                <div class="controls">
                    <input type="text" name="channel-code" placeholder="请输入渠道编号" class="mktgChnlId">
                </div>
                <div id="J-channel-code_wrapper" class="error-tips fn-hide mt-15">
                    <p id="J-channel-code" class="error">渠道编号必须是不以0开头的数字</p>
                </div>
            </div>
            <!--渠道编号  end-->
             <!--创建时间 beg-->
                        <div class="control-group">
                            <div class="control-label">
                                <em class="con-required">*</em>
                                创建时间:
                            </div>
                            <div class="controls disable-control">
                                <input type="text" class="createTime" id="createTime" value="" disabled>
                            </div>
                        </div>
                        <!--创建时间  end-->
                        <!--创建人 beg-->
                        <div class="control-group">
                            <div class="control-label">
                                <em class="con-required">*</em>
                                创建人:
                            </div>
                            <div class="controls disable-control">
                                <input type="text" value="" disabled id="founder">
                            </div>
                        </div>
                        <!--创建人  end-->
                        <!--渠道负责人 beg-->
                                    <div class="control-group">
                                        <div class="control-label">
                                            <em class="con-required">*</em>
                                            渠道负责人:
                                        </div>
                                        <div class="controls">
                                            <input type="text" placeholder="请输入渠道负责人" class="chnlRspderNm">
                                        </div>
                                        <div id="JC-responsible_wrapper" class="error-tips fn-hide mt-15">
                                            <p id="JC-responsible" class="error">请输入渠道负责人</p>
                                        </div>
                                    </div>
                                    <!--渠道负责人  end-->
                                    <!--联系方式 beg-->
                                    <div class="control-group">
                                        <div class="control-label">
                                            <em class="con-required">*</em>
                                            联系方式:
                                        </div>
                                        <div class="controls">
                                            <input type="text" placeholder="请输入联系方式" class="chnlRspderTelnum">
                                        </div>
                                        <div id="JC-phone_wrapper" class="error-tips fn-hide mt-15">
                                            <p id="JC-phone" class="error">请输入正确的联系方式</p>
                                        </div>
                                    </div>
                                    <!--联系方式  end-->
                                </form>
                              <!--  <div class="btn-box">
                                    <a href="javascript:;" class="btn btn-blue">确定提交</a>
                                    <a href="javascript:;" class="btn">取消</a>
                                </div> -->
                            </div>
                        </div>
                        <!--右侧主体内容end-->
